#include <stdio.h>
#define SIZE 5

int queue[SIZE];
int front = -1;
int rear = -1;

// Function to return the front element without removing it
void peek() {
    if (front == -1 || front > rear) {
        printf("Queue is empty! No element to peek.\n");
    } else {
        printf("Front element is: %d\n", queue[front]);
    }
}

// Function to add an element (Enqueue)
void enqueue(int value) {
    if (rear == SIZE - 1) {
        printf("Queue Overflow! Cannot insert %d\n", value);
    } else {
        if (front == -1) front = 0; 
        rear++;
        queue[rear] = value;
        printf("Inserted %d\n", value);
    }
}

// Function to remove an element (Dequeue)
void dequeue() {
    if (front == -1 || front > rear) {
        printf("Queue Underflow! Nothing to delete\n");
    } else {
        printf("Deleted %d\n", queue[front]);
        front++;
        
        if (front > rear) {
            front = rear = -1;
        }
    }
}

void display() {
    if (front == -1) {
        printf("Queue is empty\n");
    } else {
        printf("Queue elements: ");
        for (int i = front; i <= rear; i++) {
            printf("%d ", queue[i]);
        }
        printf("\n");
    }
}

int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);
    
    display();
    
    peek(); // Should show 10
    
    dequeue();
    peek(); // Should show 20
    
    return 0;
}
