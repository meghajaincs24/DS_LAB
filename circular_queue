#include <stdio.h>
#define SIZE 5

int queue[SIZE], front = -1, rear = -1;

// Check if queue is full
int isFull() {
    return (rear + 1) % SIZE == front;
}

// Check if queue is empty
int isEmpty() {
    return front == -1;
}

// Add an element
void enqueue(int value) {
    if (isFull()) {
        printf("Overflow! Cannot insert %d\n", value);
    } else {
        if (front == -1) front = 0; // Set front on first insert
        rear = (rear + 1) % SIZE;
        queue[rear] = value;
        printf("Inserted: %d\n", value);
    }
}

// Remove an element
void dequeue() {
    if (isEmpty()) {
        printf("Underflow! Queue is empty\n");
    } else {
        printf("Deleted: %d\n", queue[front]);
        if (front == rear) { // Reset if last element is removed
            front = rear = -1;
        } else {
            front = (front + 1) % SIZE;
        }
    }
}

// Display the queue
void display() {
    if (isEmpty()) {
        printf("Queue is empty\n");
        return;
    }
    printf("Queue: ");
    int i = front;
    while (1) {
        printf("%d ", queue[i]);
        if (i == rear) break;
        i = (i + 1) % SIZE;
    }
    printf("\n");
}

int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);
    enqueue(40);
    enqueue(50);
    display();

    dequeue(); // Removes 10
    dequeue(); // Removes 20

    printf("Inserting 60 (Circular Wrap):\n");
    enqueue(60);
    display();

    return 0;
}
